{
  "name": "melectron",
  "productName": "melectron",
  "version": "0.0.2",
  "description": "Movie syncing app for many clients",
  "main": "main.js",
  "scripts": {
    "start": "ENV=DEV electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build:win": "electron-builder -w --ia32",
    "build:mac": "electron-builder -m"
  },
  "repository": "https://github.com/KenLSM/melectron",
  "keywords": [
    "Electron",
    "movie",
    "sync"
  ],
  "author": "kenlsm",
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-reload": "^1.5.0"
  },
  "build": {
    "appId": "com.melectron",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {},
  "private": true
}
